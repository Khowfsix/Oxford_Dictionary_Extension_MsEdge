{
	"manifest_version": 2,
	"name": "Oxford Dictionary Lookup for Edge",
	"version": "1.0",
	"description": "Look up selected words in the Oxford Dictionary",
	"permissions": ["activeTab", "contextMenus"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"commands": {
		"lookup-word": {
			"suggested_key": {
				"default": "Ctrl+Q"
			},
			"description": "Look up dictionary or translate"
		}
	}
}
